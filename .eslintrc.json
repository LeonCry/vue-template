{
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "@vue/eslint-config-typescript",
    "@vue/eslint-config-prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["@typescript-eslint", "vue", "only-warn"],
  "rules": {
    "@typescript-eslint/no-explicit-any": ["off"],
    "vue/multi-word-component-names": "off"
  }
}
