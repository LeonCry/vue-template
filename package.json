    "element-plus": "^2.9.2",
    "pinia": "^2.2.6",
    "postcss-preset-env": "^9.6.0",
    "radash": "^12.1.0",
    "tailwindcss": "^3.4.14",
    "unplugin-auto-import": "^0.17.8",
    "unplugin-vue-components": "^0.26.0",
    "vue": "^3.5.12",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/node": "^20.17.6",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.1",
